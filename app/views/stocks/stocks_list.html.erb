<p id="notice"><%= notice %></p>
<div class="row" > 
<h1>Movimentação da Bolsa de Valores</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Receita Inicial</th>
      <th>Receira Atual</th>
      <th>Quantidade</th>
      <th>Receita Bruta</th>
      <th>Receita Bruta</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
    <% @stocks.each do |stock| %>
      <tr>
        <td><%= stock.name %></td>
        <td><%= stock.initial_income %></td>
        <td><%= stock.current_income %></td>
        <td><%= stock.amount %></td>
        <td><%= stock.gross_income %></td>
        <td><%= stock.liquid_income %></td> 
      </tr>
    <% end %>
     
  </tbody>
</table>

<br>